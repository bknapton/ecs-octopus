{
  "family": "octopus-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::665040277244:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::665040277244:role/ecsOctopusTaskRole",
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "octopus-container",
      "image": "REPLACE_ME",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "ACCEPT_EULA",
          "value": "Y"
        },
        {
          "name": "OCTOPUS_ADMIN_USERNAME",
          "value": "admin"
        },
        {
          "name": "OCTOPUS_PORT",
          "value": "8080"
        }
      ]
    }
  ]
}

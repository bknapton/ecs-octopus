{
  "family": "octopus-deploy",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::665040277244:role/ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "octopus-deploy",
      "image": "665040277244.dkr.ecr.us-east-1.amazonaws.com/octopus-deploy:latest",
      "essential": true,
      "environment": [
        {
          "name": "ACCEPT_OCTOPUS_EULA",
          "value": "Y"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        },
        {
          "containerPort": 10943,
          "hostPort": 10943
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/octopus-deploy",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
